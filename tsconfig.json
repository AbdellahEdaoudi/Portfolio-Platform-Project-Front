{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "lib/nextAuth.ts", "app/api/auth/[...nextauth]/route.ts", "app/api/sendEmail/route.js", "app/api/send-email/route.js", "app/layout.js", "app/Edit/UpdateProfileForm.jsx", "app/Edit/ArrayFieldEditor.jsx", "app/Edit/EducationEditor.jsx", "app/Edit/ExperienceEditor.jsx", "app/Edit/ProjectsEditor.jsx", "app/(me)/UserProfileDisplay.jsx", "app/portfolio/themes/theme-two.jsx", "app/portfolio/themes/theme-five.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
